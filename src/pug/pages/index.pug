extends ../layout/main
block variable
block meta
	title= 'Riverside 1'
	meta(name='description', content='')
block head
		link(rel='stylesheet', href="./assets/styles/main.min.css")
block scripts
	
	script(defer src='./assets/scripts/libs.js')
	script(defer src='https://cdnjs.cloudflare.com/ajax/libs/gsap/3.3.0/gsap.min.js')
	<script defer src="https://cdnjs.cloudflare.com/ajax/libs/jquery.imagesloaded/3.2.0/imagesloaded.pkgd.min.js"></script>
	script(defer src='./assets/scripts/pixi.min.js')
	script(defer src='./assets/scripts/canvas_slideshow.js')
	script(defer src='./assets/scripts/displacment-hover/three.min.js')
	script(defer src='./assets/scripts/displacment-hover/hover.js')
	script( defer src='./assets/scripts/home.js')
	script(defer src='./assets/scripts/index.js')
	script(defer src='./assets/scripts/gallery.js')


	
block content
	section.main-screen-layout
		.slide-wrapper
			.slide-item
				h3.slide-item__title Slide 1
				//- +img('main1.jpg').slide-item__image
				img( src="./assets/images/main1.jpg" data-src="./assets/images/main1.jpg").slide-item__image
			.slide-item
				h3.slide-item__title Slide 2
				//- +img('main2.jpg').slide-item__image
				img(src="./assets/images/main2.jpg" data-src="./assets/images/main2.jpg").slide-item__image
			.slide-item
				h3.slide-item__title Slide 3
				//- +img('main3.jpg').slide-item__image
				img(src="./assets/images/main3.jpg" data-src="./assets/images/main3.jpg").slide-item__image
			.slide-item
				h3.slide-item__title Slide 3
				//- +img('main4.jpg').slide-item__image
				img(src="./assets/images/main4.jpg" data-src="./assets/images/main4.jpg").slide-item__image
		.main-screen.wrapper
			+wave-bottom()
			.arrows
				.arrow-prev.scene-nav.scene-nav--prev(data-nav="previous")
					<svg width="22" height="20" viewBox="0 0 22 20" fill="none" xmlns="http://www.w3.org/2000/svg">
					<rect x="20" y="11" width="20" height="2" transform="rotate(-180 20 11)" fill="white"/>
					<path d="M11 19L20 10L11 0.999998" stroke="white" stroke-width="2"/>
					</svg>
				.arrow-next.scene-nav.scene-nav--next(data-nav="next")
					<svg width="22" height="20" viewBox="0 0 22 20" fill="none" xmlns="http://www.w3.org/2000/svg">
					<rect x="20" y="11" width="20" height="2" transform="rotate(-180 20 11)" fill="white"/>
					<path d="M11 19L20 10L11 0.999998" stroke="white" stroke-width="2"/>
					</svg>
			.scroll-element 
				<svg width="16" height="26" viewBox="0 0 16 26" fill="none" xmlns="http://www.w3.org/2000/svg">
				<rect x="1" y="1" width="14" height="24" rx="7" stroke="white" stroke-width="2"/>
				<rect x="7" y="12" width="2" height="5" rx="1" fill="white" class="bounce-infinite"/>
				</svg>
				.slider-counter
					.current 1
					.delimiter 
						<svg width="2" height="12" viewBox="0 0 2 12" fill="none" xmlns="http://www.w3.org/2000/svg">
						<rect opacity="0.5" width="2" height="12" transform="matrix(-1 0 0 1 2 0)" fill="white"/>
						</svg>
					.all 4
			<div class="title"> Твоє <span class="blue">правильне</span> середовище
			</div>
	section.genplan.layout
		.genplan-svg
			include ../../assets/images/genplan/svg/genplan.svg
		+wave-bottom()
		.genplan__content
			.title <span class="blue">генплан</span> комплексу
			ul.genplan-list
				li(data-svgId="football-field").svg-link-js
					span Стадіон
				li(data-svgId="skate-park").svg-link-js
					span Скейтпарк
				li(data-svgId="tennis-courts").svg-link-js
