	section.about.layout
		.left.advantages
			+wave-top()
			.left__content
				.subtitle про комплекс
				.title Твоє <span class="blue">правильне</span> середовище
				p.text Один день на курорті - це класний краєвид з вікон, ресторани в двох хвилинах, завжди свіже повітря, все необхідне під боком: спорт, концерти, променад, пляж і величезна зелена зона.
				a(href="#").link Детальніше
			.distortion-hover__item-img(data-displacement="./assets/images/about-hover-block/displacment.png" data-intensity="-0.65" )
				+img('about-hover-block/1st.jpg')
				+img('about-hover-block/2nd.jpg')
			</div>
			.right
				.title-group
					.subtitle.gray Переваги
					.title <span class="gray">Бути в</span> <span class="blue"> моменті</span>
				.text-group
					.text.gray Один день на курорті - це класний краєвид з вікон, ресторани в двох хвилинах, завжди свіже повітря, все необхідне під боком: спорт, концерти, променад, пляж і величезна зелена зона.
					a(href="#").link.blue Детальніше
				+img('about-hover-block/about-right-block.jpg').bg-image
	section.reasons-to-choose
		.reasons-to-choose__block
			.title <span class="blue with-waves">Ріверсайд</span> 
				span.gray тому що...
			//- +img('reasons-to-choose/1.jpg')
			video(poster="./assets/images/reasons-to-choose/1.jpg"  loop="true" muted playsinline )
				source(src="./static/video/SPORT.mp4" type="video/mp4")
			+wave-bottom-white('Місце, де панує спорт','Твоє правильне середовище')
		.reasons-to-choose__block
			//- +img('reasons-to-choose/2.jpg')
			video(poster="./assets/images/reasons-to-choose/2.jpg" loop="true" muted playsinline )
				source(src="./static/video/rest.mp4" type="video/mp4")
			+wave-bottom-white('простір для дозвілля','Твоє правильне середовище')
		.reasons-to-choose__block
			//- +img('reasons-to-choose/3.jpg')
			video(poster="./assets/images/reasons-to-choose/3.jpg"  loop="true" muted playsinline )
				source(src="./static/video/water.mp4" type="video/mp4")
			+wave-bottom-white('Водний простір','Твоє правильне середовище')
		.reasons-to-choose__block
			//- +img('reasons-to-choose/4.jpg')
			video(poster="./assets/images/reasons-to-choose/4.jpg"  loop="true" muted playsinline )
				source(src="./static/video/life.mp4" type="video/mp4")
			+wave-bottom-white('досконалий у дрібницях','Твоє правильне середовище')
		.reasons-to-choose__block
			//- +img('reasons-to-choose/5.jpg')
			video(poster="./assets/images/reasons-to-choose/5.jpg" loop="true" muted playsinline )
				source(src="./static/video/KIDS.mp4" type="video/mp4")
			+wave-bottom-white('Зростай і розвивайся','Твоє правильне середовище')
