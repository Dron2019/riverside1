extends ../layout/main
block variable
    -pageName = 'choose-flat';
    - var version = "ua";
    - var autoplayVideo = 'true';
block meta
    title= 'Документи Riverside1'
    meta(name='description', content='')
block head
    link(rel='stylesheet', href="./assets/styles/main.min.css")
block scripts
    script(defer src='./assets/scripts/libs.js')
    script(defer src='./assets/scripts/forms-handler.js')
    script(defer src='./assets/scripts/menu.js')
    script(defer src='./assets/scripts/choose-flat.js')
